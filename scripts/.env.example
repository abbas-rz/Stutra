# Environment variables for Stutra Database Manager
# Copy this file to .env and fill in your Firebase configuration

# Firebase Realtime Database URL
# Get this from Firebase Console > Project Settings > General > Your apps > Firebase SDK snippet
VITE_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.firebaseio.com/

# Optional: Other Firebase config (for future use)
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
